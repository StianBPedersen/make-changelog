<div class="l-changelog-header theme-margin" role="banner">
	{{#if @blog.logo}}
		<a href="{{@blog.url}}" class="logo-tp"><img src="{{@blog.logo}}" alt="{{@blog.title}}"></a>
	{{/if}}

	{{#if @blog.title}}
	<h1 class="changelog-blogtitle">{{@blog.title}}</h1>
	{{/if}}
	{{#if @blog.description}}
	<p class="changelog-blogdescription">&ndash;&nbsp;{{@blog.description}}</p>
	{{/if}}
</div>